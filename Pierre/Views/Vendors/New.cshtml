@{
  Layout = "_Layout";
}
<div id="vendorForm">
  <form action="/vendors" method="post">
    <h2>New Vendor</h2>
    <label for="vendorName"></label>
    <input id="vendorName" name="vendorName" type="text" placeholder="Name">
    <br>
    <label for="vendorLocation"></label>
    <input id="vendorLocation" name="vendorLocation" type="text" placeholder="Location">
    <br>
    <button type="submit">Submit</button>
  </form>
  <button><a href="/vendors">Return to Vendors List</a></button>
</div>
